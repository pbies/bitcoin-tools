./monero-wallet-rpc \
  --rpc-bind-port 18083 \
  --rpc-bind-ip 127.0.0.1 \
  --disable-rpc-login \
  --wallet-dir ./xmr_watch_wallets \
  --daemon-address http://127.0.0.1:18081 \
  --disable-rpc-ban

